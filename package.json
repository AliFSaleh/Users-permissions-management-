  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "tsc -p .",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d src/utils/data-source.ts",
