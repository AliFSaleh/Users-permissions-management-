    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/config": "^3.3.4",
    "@types/express": "^4.17.21",
    "@types/nodemon": "^1.19.6",
    "cors": "^2.8.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0"
